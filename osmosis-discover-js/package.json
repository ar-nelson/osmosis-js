{
  "name": "@nels.onl/osmosis-discover-js",
  "version": "0.1",
  "description": "UDP service discovery and pairing for Osmosis",
  "main": "dist/index.js",
  "author": "Adam Nelson <adam@nels.onl>",
  "license": "BlueOak-1.0.0",
  "scripts": {
    "build.proto": "./build-proto.sh",
    "build": "tsc && babel src --out-dir dist --extensions .ts && cp -f src/*.js dist/",
    "clean": "rm -rf dist build",
    "lint": "eslint --ext ts src",
    "prestart": "yarn build",
    "start": "node --unhandled-rejections=strict dist/index.js 99804c7c-2abb-42ee-94e8-2a5d6cbc5f55 | bunyan -o short",
    "test": "rm -f test.log; mocha || bunyan --no-pager -o short test.log"
  },
  "dependencies": {
    "broadcast-interfaces": "^1.0.0",
    "bunyan": "^1.8.14",
    "get-port": "^5.1.1",
    "lodash.isplainobject": "^4.0.6",
    "sodium-native": "^3.2.0",
    "uuid": "^8.3.1",
    "zstd-codec": "^0.1.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.8",
    "@babel/core": "^7.12.9",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-typescript": "^7.12.7",
    "@babel/register": "^7.12.1",
    "@types/bunyan": "^1.8.6",
    "@types/chai": "^4.2.14",
    "@types/chai-as-promised": "^7.1.3",
    "@types/chance": "^1.1.1",
    "@types/lodash.isplainobject": "^4.0.6",
    "@types/mocha": "^8.2.0",
    "@types/node": "^14.14.10",
    "@types/sodium-native": "^2.3.5",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chance": "^1.1.7",
    "eslint": "^7.14.0",
    "mocha": "^8.2.1",
    "prettier": "^2.2.1",
    "prettier-plugin-organize-imports": "^1.1.1",
    "ts-protoc-gen": "^0.13.0",
    "typescript": "^4.1.2"
  }
}
