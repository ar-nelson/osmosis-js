{
  "name": "@nels.onl/osmosis-store-js",
  "version": "0.1.0",
  "description": "Mergeable CRDT-based reactive JSON state management",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "Adam Nelson <adam@nels.onl>",
  "license": "BlueOak-1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/ar-nelson/osmosis-js.git",
    "directory": "osmosis-store-js"
  },
  "scripts": {
    "build.grammar": "nearleyc src/jsonpath.grammar.ne -o src/jsonpath.grammar.ts && eslint --quiet --fix src/jsonpath.grammar.ts && prettier -w --parser=babel src/jsonpath.grammar.ts",
    "prebuild": "yarn build.grammar",
    "build": "tsc && babel src --out-dir dist --extensions .ts",
    "lint": "eslint --ext ts src",
    "clean": "rm -rf dist",
    "test": "mocha"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
